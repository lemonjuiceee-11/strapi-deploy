{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Orders",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "totalOrderAmount": {
      "type": "decimal"
    },
    "userId": {
      "type": "integer"
    },
    "paymentId": {
      "type": "string"
    },
    "orderItemList": {
      "type": "component",
      "repeatable": true,
      "component": "ordered-item.ordered-item"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Confirmed",
        "To Ship",
        "Delivered",
        "Completed",
        "Cancelled",
        "Returns"
      ],
      "default": "Pending"
    },
    "email": {
      "type": "email"
    },
    "paymentMethod": {
      "type": "string"
    },
    "delivery_assignment": {
      "type": "enumeration",
      "enum": [
        "Delivery 1",
        "Delivery 2",
        "Delivery 3"
      ]
    },
    "notes": {
      "type": "text"
    },
    "refund_reason": {
      "type": "text"
    },
    "refund_method": {
      "type": "string"
    },
    "account_number": {
      "type": "biginteger"
    },
    "refund_proof": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "delivery_proof": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
